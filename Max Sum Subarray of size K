// OYO Rooms

class Solution {
    public int maxSubarraySum(int[] arr, int k) {
        // Code here
        int n = arr.Length;

        // Compute sum of first k elements
        int sum = 0;
        for (int i = 0; i < k; i++) {
            sum += arr[i];
        }

        int maxSum = sum;

        // Slide the window
        for (int i = k; i < n; i++) {
            sum += arr[i] - arr[i - k];
            if (sum > maxSum) {
                maxSum = sum;
            }
        }

        return maxSum;

    }
}
